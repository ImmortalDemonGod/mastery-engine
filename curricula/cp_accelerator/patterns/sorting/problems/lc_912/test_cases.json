{
  "problem": "Sort an Array",
  "source": "https://leetcode.com/problems/sort-an-array/",
  "note": "These are example test cases from the problem statement. Full validation requires online judge submission.",
  "tests": [
    {
      "id": 1,
      "input": {
        "nums": [5, 2, 3, 1]
      },
      "expected": [1, 2, 3, 5],
      "note": "Example 1 from problem statement"
    },
    {
      "id": 2,
      "input": {
        "nums": [5, 1, 1, 2, 0, 0]
      },
      "expected": [0, 0, 1, 1, 2, 5],
      "note": "Example 2 from problem statement"
    },
    {
      "id": 3,
      "input": {
        "nums": [3]
      },
      "expected": [3],
      "note": "Single element"
    },
    {
      "id": 4,
      "input": {
        "nums": []
      },
      "expected": [],
      "note": "Empty array"
    },
    {
      "id": 5,
      "input": {
        "nums": [5, 4, 3, 2, 1]
      },
      "expected": [1, 2, 3, 4, 5],
      "note": "Reverse sorted"
    },
    {
      "id": 6,
      "input": {
        "nums": [1, 2, 3, 4, 5]
      },
      "expected": [1, 2, 3, 4, 5],
      "note": "Already sorted"
    },
    {
      "id": 7,
      "input": {
        "nums": [-4, 0, 7, 4, 9, -5, -1, 0, -7, -1]
      },
      "expected": [-7, -5, -4, -1, -1, 0, 0, 4, 7, 9],
      "note": "With negative numbers and duplicates"
    }
  ]
}