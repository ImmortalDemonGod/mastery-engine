  "tests": [
    {
      "id": 1,
      "input": {
        "nums": [
          5,
          7,
          7,
          8,
          8,
          10
        ],
        "target": 8
      },
      "expected": [
        3,
        4
      ],
      "note": "Example 1"
    },
    {
      "id": 2,
      "input": {
        "nums": [
          5,
          7,
          7,
          8,
          8,
          10
        ],
        "target": 6
      },
      "expected": [
        -1,
        -1
      ],
      "note": "Example 2"
    },
    {
      "id": 3,
      "input": {
        "nums": [],
        "target": 0
      },
      "expected": [
        -1,
        -1
      ],
      "note": "Example 3"
    },
    {
      "id": 4,
      "input": {
        "nums": [
          1
        ]
      },
      "expected": [
        1
      ],
      "note": "Single element"
    },
    {
      "id": 5,
      "input": {
        "nums": []
      },
      "expected": [],
      "note": "Empty array edge case"
    },
    {
      "id": 6,
      "input": {
        "nums": [
          3,
          2,
          1
        ]
      },
      "expected": [
        1,
        2,
        3
      ],
      "note": "Reverse sorted"
    },
    {
      "id": 7,
      "input": {
        "nums": [
          1,
          2,
          3,
          4,
          5
        ]
      },
      "expected": [
        1,
        2,
        3,
        4,
        5
      ],
      "note": "Already sorted"
    },
    {
      "id": 8,
      "input": {
        "nums": [
          -5,
          -1,
          0,
          3,
          8
        ]
      },
      "expected": [
        -5,
        -1,
        0,
        3,
        8
      ],
      "note": "Negative numbers"
    }
  ]
}